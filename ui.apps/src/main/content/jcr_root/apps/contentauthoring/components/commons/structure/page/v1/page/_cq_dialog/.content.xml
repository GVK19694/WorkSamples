<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0"
          xmlns:nt="http://www.jcp.org/jcr/nt/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0"
          xmlns:granite="http://www.adobe.com/jcr/granite/1.0"
          jcr:primaryType="nt:unstructured">
    <content jcr:primaryType="nt:unstructured">
        <items jcr:primaryType="nt:unstructured">
            <tabs jcr:primaryType="nt:unstructured">
                <items jcr:primaryType="nt:unstructured" sling:hideChildren="[socialmedia,pwa,cloudservices,personalization,permissions,blueprint,livecopy]">
                    <basic jcr:primaryType="nt:unstructured">
                        <items jcr:primaryType="nt:unstructured">
                            <column jcr:primaryType="nt:unstructured">
                                <items jcr:primaryType="nt:unstructured" sling:hideChildren="[branding,htmlid,moretitles,onofftime,vanityurl]">
                                    <title jcr:primaryType="nt:unstructured"
                                           jcr:title="Title"
                                           sling:resourceType="granite/ui/components/coral/foundation/form/fieldset">
                                        <items jcr:primaryType="nt:unstructured">
                                            <title jcr:primaryType="nt:unstructured"
                                                   path="/mnt/overlay/wcm/foundation/components/basicpage/v1/basicpage/tabs/basic/items/column/items/title/items/title"
                                                   sling:resourceType="granite/ui/components/coral/foundation/include"/>
                                            <pagename jcr:primaryType="nt:unstructured"
                                                      path="/mnt/overlay/wcm/foundation/components/basicpage/v1/basicpage/tabs/basic/items/column/items/title/items/pagename"
                                                      sling:resourceType="granite/ui/components/coral/foundation/include"/>
                                        </items>
                                    </title>
                                </items>
                            </column>
                        </items>
                    </basic>
                    <advanced jcr:primaryType="nt:unstructured"
                              cq:showOnCreate="{Boolean}false"
                              sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns"
                              jcr:title="Advanced">
                        <items jcr:primaryType="nt:unstructured">
                            <column jcr:primaryType="nt:unstructured"
                                    sling:resourceType="granite/ui/components/coral/foundation/container">
                                <items jcr:primaryType="nt:unstructured">
                                    <configuration jcr:primaryType="nt:unstructured"
                                                   path="/mnt/overlay/wcm/foundation/components/basicpage/v1/basicpage/tabs/advanced/items/column/items/configuration"
                                                   sling:resourceType="granite/ui/components/coral/foundation/include"/>
                                    <templates jcr:primaryType="nt:unstructured"
                                               path="/mnt/overlay/wcm/foundation/components/basicpage/v1/basicpage/tabs/advanced/items/column/items/templates"
                                               sling:resourceType="granite/ui/components/coral/foundation/include"/>
                                </items>
                            </column>
                        </items>
                    </advanced>
                </items>
            </tabs>
        </items>
    </content>
</jcr:root>
