<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0"
          xmlns:cq="http://www.day.com/jcr/cq/1.0"
          xmlns:jcr="http://www.jcp.org/jcr/1.0"
          xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
          jcr:primaryType="nt:unstructured">
    <container
        jcr:primaryType="nt:unstructured"
        sling:resourceType="contentauthoring/components/ivcompatibility/layout/primary-drug-container"
        layout="responsiveGrid">
        <concentration
            jcr:primaryType="nt:unstructured"
            sling:resourceType="contentauthoring/components/ivcompatibility/content/concentration">
            <container
                jcr:primaryType="nt:unstructured"
                sling:resourceType="contentauthoring/components/ivcompatibility/layout/concentration-container"
                layout="responsiveGrid">
                <compatibility
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="contentauthoring/components/ivcompatibility/content/compatibility">
                    <container
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="contentauthoring/components/ivcompatibility/layout/compatibility-container"
                        layout="responsiveGrid">
                        <secondary_drug
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="contentauthoring/components/ivcompatibility/content/secondary-drug"/>
                    </container>
                </compatibility>
            </container>
        </concentration>
    </container>
</jcr:root>
